<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- 导入按钮公共配置文件 -->
	<import resource="classpath:nc/ui/pmpub/config/action_config.xml" />
	<import resource="classpath:nc/ui/pmpub/config/frame_config.xml" />
	<!-- 分页倒入文件 -->
	<import resource="classpath:nc/ui/pm/config/pagebar_config.xml" />
	
	<bean id="context" class="nc.vo.uif2.LoginContext" />

	<bean id="boadatorfactory"
		class="nc.ui.pubapp.uif2app.view.value.AggVOMetaBDObjectAdapterFactory" />

	<bean id="manageAppModel" class="nc.ui.pm.model.BillManageModel">
		<constructor-arg ref="context"></constructor-arg>
		<property name="businessObjectAdapterFactory" ref="boadatorfactory" />
		<property name="context" ref="context" />
		<property name="billType" value="4D18" />
		<property name="last_v_flag" value="last_v_flag" />
	</bean>

	<bean id="modelDataManager" class="nc.ui.pm.model.PMPaginationModelDataManager">
		<property name="model" ref="manageAppModel"></property>
		<property name="paginationModel" ref="paginationModel" />
		<property name="pageQueryService" ref="pageQueryService" />
	</bean>

	<bean id="templateContainer" class="nc.ui.uif2.editor.TemplateContainer">
		<property name="context" ref="context" />
		<!-- <property name="nodeKeies"> <list> <value>4810012015</value> </list> 
			</property> -->
	</bean>

	<bean id="queryTemplateContainer" class="nc.ui.uif2.editor.QueryTemplateContainer">
		<property name="context" ref="context" />
	</bean>
	<!-- 列表管理数据 -->
	<bean id="listViewValueStrategy" class="nc.ui.pm.model.PMBillListValueAdapter">
		<property name="VONameToSortFileds">
			<map>
				<entry>
					<!--页签 VO类名 -->
					<key>
						<value>nc.vo.pbm.budget.BudgetCBSBodyVO</value>
					</key>
					<!--该页签 要排序的字段 -->
					<list>
						<value>pk_cbsnode.code</value>
					</list>
				</entry>

				<entry>
					<!--页签 VO类名 -->
					<key>
						<value>nc.vo.pbm.budget.BudgetFactorBodyVO</value>
					</key>
					<!--该页签 要排序的字段 -->
					<list>
						<value>pk_cbsnode.code</value>
						<value>pk_factor.factorcode</value>
					</list>
				</entry>
			</map>
		</property>
	</bean>

	<bean id="listView" class="nc.ui.pmpub.view.PMBillListView"
		init-method="initUI">
		<property name="billListPanelValueSetter" ref="listViewValueStrategy" />
		<property name="paginationBar" ref="paginationBar" />
		<property name="model" ref="manageAppModel" />
		<property name="templateContainer" ref="templateContainer" />
		<property name="userdefitemListPreparator">
			<bean class="nc.ui.pubapp.uif2app.view.CompositeBillListDataPrepare">
				<property name="billListDataPrepares">
					<list>
						<ref bean="userdefitemlistPreparator" />
					</list>
				</property>
			</bean>
		</property>
		<property name="showTotalLine" value="true"></property>
		<property name="showTotalLineTabcodes">
			<list>
				<value>budgetcbs</value>
				<value>budgetfactor</value>
			</list>
		</property>
	</bean>

	<bean id="billCardValueStrategy" class="nc.ui.pm.model.PMBillCardValueAdapter">
		<!-- 指定每个页签的 排序字段 -->
		<property name="VONameToSortFileds">
			<map>
				<entry>
					<!--页签 VO类名 -->
					<key>
						<value>nc.vo.pbm.budget.BudgetCBSBodyVO</value>
					</key>
					<!--该页签 要排序的字段 -->
					<list>
						<value>pk_cbsnode.code</value>
					</list>
				</entry>

				<entry>
					<!--页签 VO类名 -->
					<key>
						<value>nc.vo.pbm.budget.BudgetFactorBodyVO</value>
					</key>
					<!--该页签 要排序的字段 -->
					<list>
						<value>pk_cbsnode.code</value>
						<value>pk_factor.factorcode</value>
					</list>
				</entry>
			</map>
		</property>
	</bean>

	<bean id="billFormEditor" class="nc.ui.pmpub.view.PMBillFormForProject"
		init-method="initUI">
		<property name="model" ref="manageAppModel" />
		<property name="templateContainer" ref="templateContainer" />
		<property name="showOrgPanel" value="true" />
		<property name="componentValueManager" ref="billCardValueStrategy" />
		<property name="blankChildrenFilter">
			<bean class="nc.ui.pm.filter.PMMultiFieldsBlankChildrenFilter">
				<!-- 指定用来判断空行的字段名称，可能多数情况下是存货字段 -->
				<property name="filterMap">
					<map>
						<entry>
							<!--页签code -->
							<key>
								<value>budgetcbs</value>
							</key>
							<!--检验是否空行字段code -->
							<list>
								<value>pk_cbsnode</value>
							</list>
						</entry>
						<entry>
							<!--页签code -->
							<key>
								<value>budgetfactor</value>
							</key>
							<!--检验是否空行字段code -->
							<list>
								<value>pk_cbsnode</value>
								<value>pk_factor</value>
							</list>
						</entry>
					</map>
				</property>
			</bean>
		</property>
		<property name="autoAddLine" value="false" />
		<property name="showTotalLineTabcodes">
			<list>
				<value>budgetcbs</value>
				<value>budgetfactor</value>
			</list>
		</property>
		<property name="bodyActionMap">
			<map>
				<entry key="budgetcbs">
					<list>
						<!-- 提供了一组默认实现 -->
						<bean class="nc.ui.pubapp.uif2app.actions.BodyAddLineAction" />
						<bean class="nc.ui.pubapp.uif2app.actions.BodyInsertLineAction" />
						<bean class="nc.ui.pbm.budget.tab.action.CBSDelLineAction" />
						<bean class="nc.ui.pubapp.uif2app.actions.BodyLineEditAction" />
						<bean class="nc.ui.pub.beans.ActionsBar$ActionsBarSeparator" />
						<bean class="nc.ui.pm.action.BillViewMaxAction">
							<property name="billForm" ref="billFormEditor" />
							<property name="pos" value="1" />
						</bean>

					</list>
				</entry>
				<entry key="budgetfactor">
					<list>
						<!-- 提供了一组默认实现 -->
						<bean class="nc.ui.pubapp.uif2app.actions.BodyAddLineAction" />
						<bean class="nc.ui.pbm.budget.tab.action.FactorInsertLineAction" />
						<bean class="nc.ui.pbm.budget.tab.action.FactorDelLineAction" />
						<bean class="nc.ui.pubapp.uif2app.actions.BodyLineEditAction" />
						<bean class="nc.ui.pub.beans.ActionsBar$ActionsBarSeparator" />
						<bean class="nc.ui.pm.action.BillViewMaxAction">
							<property name="billForm" ref="billFormEditor" />
							<property name="pos" value="1" />
						</bean>

					</list>
				</entry>
			</map>
		</property>
		<property name="userdefitemPreparator">
			<bean class="nc.ui.pubapp.uif2app.view.CompositeBillDataPrepare">
				<property name="billDataPrepares">
					<list>
						<ref bean="userdefitemPreparator" />
						<ref bean="marAsstPreparator" />
					</list>
				</property>
			</bean>
		</property>

	</bean>
	<bean id="userdefitemlistPreparator" class="nc.ui.uif2.editor.UserdefitemContainerListPreparator">
		<property name="container" ref="userdefitemContainer" />
		<property name="params">
			<list>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pim.budgetheadvo" />
					<property name="rulecode" value="4D18_H" />
					<property name="pos" value="0" />
					<property name="prefix" value="hdef" />
				</bean>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pim.budgetcbs" />
					<property name="rulecode" value="4D18_CBS" />
					<property name="pos" value="1" />
					<property name="prefix" value="bdef" />
					<property name="tabcode" value="budgetcbs" />
				</bean>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pim.budgetfactor" />
					<property name="rulecode" value="4D18_Factor" />
					<property name="pos" value="1" />
					<property name="prefix" value="bdef" />
					<property name="tabcode" value="budgetfactor" />
				</bean>
			</list>
		</property>
	</bean>

	<bean id="userdefitemPreparator" class="nc.ui.uif2.editor.UserdefitemContainerPreparator">
		<property name="container" ref="userdefitemContainer" />
		<property name="params">
			<list>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pim.budgetheadvo" />
					<property name="rulecode" value="4D18_H" />
					<property name="pos" value="0" />
					<property name="prefix" value="hdef" />
				</bean>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pim.budgetcbs" />
					<property name="rulecode" value="4D18_CBS" />
					<property name="pos" value="1" />
					<property name="prefix" value="bdef" />
					<property name="tabcode" value="budgetcbs" />
				</bean>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pim.budgetfactor" />
					<property name="rulecode" value="4D18_Factor" />
					<property name="pos" value="1" />
					<property name="prefix" value="bdef" />
					<property name="tabcode" value="budgetfactor" />
				</bean>
			</list>
		</property>
	</bean>

	<bean id="marAsstPreparator"
		class="nc.ui.pubapp.uif2app.view.material.assistant.MarAsstPreparator">
		<property name="model" ref="manageAppModel" />
		<property name="container" ref="userdefitemContainer" />
		<property name="prefix" value="vfree" />
	</bean>

	<bean id="userdefitemContainer" class="nc.ui.uif2.userdefitem.UserDefItemContainer">
		<property name="context" ref="context" />
		<property name="params">
			<list>
				<bean class="nc.ui.uif2.userdefitem.QueryParam">
					<property name="mdfullname" value="pim.budgetcreate" />
					<property name="rulecode" value="4D18_H" />
				</bean>
				<bean class="nc.ui.uif2.userdefitem.QueryParam">
					<property name="mdfullname" value="pim.budgetcbs" />
					<property name="rulecode" value="4D18_CBS" />
				</bean>
				<bean class="nc.ui.uif2.userdefitem.QueryParam">
					<property name="mdfullname" value="pim.budgetfactor" />
					<property name="rulecode" value="4D18_Factor" />
				</bean>
			</list>
		</property>
	</bean>

	<bean id="queryArea" class="nc.ui.pubapp.uif2app.tangramlayout.UEQueryAreaShell"
		init-method="initUI">
		<property name="queryAreaCreator" ref="queryAction" />
	</bean>

	<bean id="queryInfo" class="nc.ui.uif2.tangramlayout.CardLayoutToolbarPanel">
		<property name="model" ref="manageAppModel" />
	</bean>

	<bean id="container" class="nc.ui.uif2.TangramContainer"
		init-method="initUI">
		<property name="model" ref="manageAppModel" />
		<property name="tangramLayoutRoot">
			<bean class="nc.ui.uif2.tangramlayout.node.TBNode">
				<property name="showMode" value="CardLayout" />
				<property name="tabs">
					<list>
						<bean class="nc.ui.uif2.tangramlayout.node.HSNode">
							<property name="left">
								<bean class="nc.ui.uif2.tangramlayout.node.CNode">
									<property name="component" ref="queryArea" />
								</bean>
							</property>

							<property name="right">
								<bean class="nc.ui.uif2.tangramlayout.node.VSNode">
									<property name="up">
										<bean class="nc.ui.uif2.tangramlayout.node.CNode">
											<property name="component" ref="queryInfo" />
										</bean>
									</property>

									<property name="down">
										<bean class="nc.ui.uif2.tangramlayout.node.CNode">
											<property name="name" value="列表" />
											<property name="component" ref="listView" />
										</bean>
									</property>

									<property name="dividerLocation" value="25f" />
								</bean>
							</property>

							<property name="dividerLocation" value="210f" />
						</bean>
						<bean class="nc.ui.uif2.tangramlayout.node.VSNode">
							<property name="up">
								<bean class="nc.ui.uif2.tangramlayout.node.CNode">
									<property name="component" ref="cardInfoPnl" />
								</bean>
							</property>

							<property name="down">
								<bean class="nc.ui.uif2.tangramlayout.node.CNode">
									<property name="name" value="卡片" />
									<property name="component" ref="billFormEditor" />
								</bean>
							</property>

							<property name="dividerLocation" value="30f" />
						</bean>
					</list>
				</property>
			</bean>
		</property>

	</bean>

	<bean id="childrenPicky" class="nc.ui.pubapp.uif2app.event.ChildrenPicky">
		<property name="billform" ref="billFormEditor" />
		<property name="bodyVoClasses">
			<list>
				<value>nc.vo.pbm.budget.BudgetCBSBodyVO</value>
				<value>nc.vo.pbm.budget.BudgetFactorBodyVO</value>
			</list>
		</property>
	</bean>

	<bean id="eventMediator" class="nc.ui.pubapp.uif2app.model.AppEventHandlerMediator">
		<property name="model" ref="manageAppModel" />
		<property name="handlerGroup">
			<list>
				<bean class="nc.ui.pubapp.uif2app.event.EventHandlerGroup">
					<property name="event"
						value="nc.ui.pubapp.uif2app.event.card.CardHeadTailBeforeEditEvent" />
					<property name="handler">
						<bean class="nc.ui.pm.disposeprojectref.HeadProjectRefBeforeEditHandler" />
					</property>
				</bean>
				<!-- 跨组织选部门编辑前事件 -->
				<bean class="nc.ui.pubapp.uif2app.event.EventHandlerGroup">
					<property name="event"
						value="nc.ui.pubapp.uif2app.event.card.CardHeadTailBeforeEditEvent" />
					<property name="handler" ref="deptAndPsnBeforeEditHandler" />
				</bean>
				<!-- 跨组织选部门编辑后事件 -->
				<bean class="nc.ui.pubapp.uif2app.event.EventHandlerGroup">
					<property name="event"
						value="nc.ui.pubapp.uif2app.event.card.CardHeadTailAfterEditEvent" />
					<property name="handler" ref="psnAfterEditHandler" />
				</bean>
			</list>
		</property>
		<property name="handlerMap">
			<map>
				<entry key="nc.ui.pubapp.uif2app.event.billform.AddEvent">
					<list>
						<bean class="nc.ui.pbm.budget.handler.AddHandler" />
					</list>
				</entry>
				<!-- 主组织(项目组织)编辑事件 -->
				<entry key="nc.ui.pubapp.uif2app.event.OrgChangedEvent">
					<list>
						<ref bean="mainorgchghandler" />
					</list>
				</entry>

				<!-- 表头(尾)编辑前事件 -->
				<entry key="nc.ui.pubapp.uif2app.event.card.CardHeadTailBeforeEditEvent">
					<list>
						<ref bean="before_headtail_edithandler" />
						<ref bean="CBSFilterHead" />
						<bean class="nc.ui.pm.handler.Pk_currtypeBeforeHandler" />
					</list>
				</entry>
				<!-- 表头(尾)编辑后事件 -->
				<entry key="nc.ui.pubapp.uif2app.event.card.CardHeadTailAfterEditEvent">
					<list>
						<ref bean="after_headtail_edithandler" />
					</list>
				</entry>
				<!-- 表体编辑前事件 -->
				<entry key="nc.ui.pubapp.uif2app.event.card.CardBodyBeforeEditEvent">
					<list>
						<ref bean="before_body_edithandler" />
						<ref bean="factorBodyBeforeHandler" />
						<ref bean="CBSFilterBody" />
					</list>
				</entry>
				<!-- 表体编辑后事件 -->
				<entry key="nc.ui.pubapp.uif2app.event.card.CardBodyAfterEditEvent">
					<list>
						<ref bean="after_body_edithandler" />
					</list>
				</entry>

				<!-- 列表界面表头行切换事件 -->
				<entry key="nc.ui.pubapp.uif2app.event.list.ListHeadRowChangedEvent">
					<list>
						<ref bean="list_headrow_chghandler" />
					</list>
				</entry>

				<!-- 列表加载数据事件 -->
				<entry key="nc.ui.pubapp.uif2app.event.list.ListPanelLoadEvent">
					<list>
						<ref bean="list_loadhandler" />
					</list>
				</entry>

			</map>
		</property>
	</bean>

	<!-- 核算要素过滤 -->
	<bean id="factorBodyBeforeHandler" class="nc.ui.pm.handler.FactorBodyBeforeEditHandler">
		<property name="factorFeild" value="pk_factor" />
		<property name="CBSNodeFiled" value="pk_cbsnode" />
	</bean>

	<!--表头 中的 CBS过滤 -->
	<bean id="CBSFilterHead" class="nc.ui.pm.handler.CBSNodeHeadBeforeEditHandler">
		<property name="CBSNodeFiled" value="pk_default_cbsnode" />
		<property name="projectIDFeild" value="pk_project" />
	</bean>
	<!--表体 中CBS过滤 -->
	<bean id="CBSFilterBody" class="nc.ui.pm.handler.CBSNodeBodyBeforeEditHandler">
		<property name="CBSNodeFiled" value="pk_cbsnode" />
		<property name="projectIDFeild" value="pk_project" />
	</bean>

	<!-- 列表加载数据事件 -->
	<bean id="list_loadhandler" class="nc.ui.pm.handler.DefaultListPanelLoadHandler">
		<property name="billList" ref="listView" />
		<property name="scaleProcess" ref="scaleProcess" />
	</bean>
	<!-- 表头行切换处理类 -->
	<bean id="list_headrow_chghandler" class="nc.ui.pm.handler.DefaultListHeadRowChangeHandler">
		<property name="scaleProcess" ref="scaleProcess" />
	</bean>

	<!-- 表头(尾)编辑后事件处理器 -->
	<bean id="after_headtail_edithandler" class="nc.ui.pbm.budget.handler.HeadTailAfterEditEventHandler">
		<property name="cardForm" ref="billFormEditor" />
		<property name="scaleProcess" ref="scaleProcess" />
	</bean>

	<!-- 精度处理事件 -->
	<bean id="scaleProcess" class="nc.ui.pbm.budget.utils.BudgetScaleUtil" />


	<!-- 主组织(项目组织)编辑后处理 -->
	<bean id="mainorgchghandler" class="nc.ui.pbm.budget.handler.OrgChangedHandler">
		<property name="cardForm" ref="billFormEditor" />
		<property name="scaleProcess" ref="scaleProcess" />
	</bean>

	<!-- 表头(尾)编辑前事件处理器 -->
	<bean id="before_headtail_edithandler" class="nc.ui.pbm.budget.handler.HeadTailBeforeEditEventHandler">
		<property name="cardForm" ref="billFormEditor" />
	</bean>

	<!-- 表体编辑前事件处理器 -->
	<bean id="before_body_edithandler" class="nc.ui.pbm.budget.handler.BodyBeforeEditEventHandler">
		<property name="refMultiFiList">
			<list>
				<value>pk_cbsnode</value>
				<value>pk_factor</value>
			</list>
		</property>
	</bean>

	<!-- 表体编辑后事件处理器 -->
	<bean id="after_body_edithandler" class="nc.ui.pbm.budget.handler.BodyAfterEditEventHandler">
		<property name="cardForm" ref="billFormEditor" />
		<property name="refMultiFiList">
			<list>
				<value>pk_cbsnode</value>
				<value>pk_factor</value>
			</list>
		</property>
	</bean>


	<bean id="toftpanelActionContributors" class="nc.ui.uif2.actions.ActionContributors">
		<property name="contributors">
			<list>
				<ref bean="actionsOfList" />
				<ref bean="actionsOfCard" />
			</list>
		</property>
	</bean>

	<bean id="actionsOfList" class="nc.ui.uif2.actions.StandAloneToftPanelActionContainer">
		<constructor-arg ref="listView" />
		<property name="model" ref="manageAppModel" />
		<property name="actions">
			<list>
				<ref bean="addAction" />
				<ref bean="editAction" />
				<ref bean="deleteAction" />
				<ref bean="separatorAction" />
				<ref bean="queryAction" />
				<ref bean="refreshAction" />
				<ref bean="separatorAction" />
				<ref bean="commitMenuAction" />
				<ref bean="auditMenuAction" />
				<ref bean="executeActionGroup" />
				<ref bean="assMenuAction" />
				<ref bean="separatorAction" />
				<ref bean="linkQueryGroup" />
				<ref bean="separatorAction" />
				<ref bean="printMenuAction" />
				<ref bean="separatorAction" />
				<ref bean="impexpActionGroup" />
			</list>
		</property>
	</bean>

	<bean id="actionsOfCard" class="nc.ui.uif2.actions.StandAloneToftPanelActionContainer">
		<constructor-arg ref="billFormEditor" />
		<property name="model" ref="manageAppModel" />
		<property name="actions">
			<list>
				<ref bean="addAction" />
				<ref bean="editAction" />
				<ref bean="deleteAction" />
				<ref bean="separatorAction" />
				<ref bean="queryAction" />
				<ref bean="cardRefreshAction" />
				<ref bean="separatorAction" />
				<ref bean="commitMenuAction" />
				<ref bean="auditMenuAction" />
				<ref bean="executeActionGroup" />
				<ref bean="assMenuAction" />
				<ref bean="separatorAction" />

				<ref bean="linkQueryGroup" />
				<ref bean="separatorAction" />
				<ref bean="impexpActionGroup" />
				<ref bean="printMenuAction" />
				<ref bean="separatorAction" />
			</list>
		</property>
		<property name="editActions">
			<list>
				<ref bean="saveAction" />
				<ref bean="saveAndCommitAction" />
				<ref bean="separatorAction" />
				<ref bean="cancelaction" />
				<ref bean="separatorAction" />
				<ref bean="budgetResourceAction" />
				<ref bean="resetAction" />
				<ref bean="separatorAction" />
